<h1>To do list</h1>

<table border = 1px style ="width:50%">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Completed</th>
    <th>User ID</th>
  </tr>

  <tr>
    <% @todos.each do |todo| %>
      <td>
        <%= todo.title %>
      </td>

      <td>
        <%= todo.description %>
      </td>

      <td>
        <%= todo.completed %>
      </td>

      <td>
        <%= todo.user_id %>
      </td>
  </tr>  
    <% end %>
</table>


<a href="/todos/new">Click here</a> to add new items.<br>
<a href="/current_user">Click here</a> to view current user info.<br>
<a href="/form_to_edit_user">Click here</a> to edit current user info.<br>
<a href="/form_to_delete_user">Click here</a> to delete current user.<br>
<a href="/logins/log_out">Click here</a> to log out.